{
    "manifest_version": 2,
    "name": "text-to-speech",
    "description": "Text to speech extension",
    "version": "1.0.0",
    "icons": { "128": "assets/icon.png" },
    "browser_action": {
        "default_icon": "assets/icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}